    ns1:definition "The postal code. For example, 94043."@en,
        "Le code postale."@fr,
        "De postcode."@nl ;
    ns4:domainIncludes ns4:DefinedRegion,
        ns4:GeoCoordinates,
        ns4:GeoShape,
        ns4:PostalAddress ;
    ns4:rangeIncludes ns4:Text .

ns4:publisher a ns5:Property ;
    ns2:label "publisher"@en,
